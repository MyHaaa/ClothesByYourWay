@model Models.EF.Supplier
@{
    ViewBag.Title = "Index";
    Layout = "~/Areas/Administrator/Views/Shared/_Layout.cshtml";
}


@section jsFooter{
    @Scripts.Render("~/bundles/jqueryval")
}


<div class="table-agile-info">
    <div class="panel panel-default">
        <div class="panel-heading">
            Danh sách các nhà phân phối
        </div>
        <div class="row w3-res-tb">
            <ul class="nav nav-tabs">
                <li class="active"><a data-toggle="tab" href="#firstTab">Tất cả</a></li>
                <li><a data-toggle="tab" href="#secondTab">Thêm mới</a></li>


            </ul>
            <div class="tab-content">
                <div id="firstTab" class="tab-pane fade in active">@Html.Action("ViewAll")</div>

                <div id="secondTab" class="tab-pane fade in">@Html.Action("AddOrEdit")</div>

            </div>
        </div>


    </div>
</div>
<script>
    function activatejQueryTable() {
        $("#deTable").DataTable();
    }

    $(function () {
        activatejQueryTable();
    });
</script>

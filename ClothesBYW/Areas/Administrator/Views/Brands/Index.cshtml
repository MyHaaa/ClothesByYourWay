@model Models.EF.Brand
@{
    ViewBag.Title = "Index";
    Layout = "~/Areas/Administrator/Views/Shared/_Layout.cshtml";
}



@section jsFooter{
    @Scripts.Render("~/bundles/jqueryval")
}

<div class="col-lg-12">
    <div class="d-flex flex-wrap align-items-center justify-content-between mb-4">
        <div>
            <h4 class="mb-3">Quản lý các thương hiệu sản phẩm</h4>
            <p class="mb-0">
                Create and manage your vendor list, send and receive purchase orders – your online<br>
                Dashboard is your new back of house.
            </p>
        </div>
    </div>
</div>
<div class="col-lg-12">
    <ul class="nav nav-tabs" role="tablist">
        <li class="nav-item"><a class="nav-link active" id="home-tab" data-toggle="tab" role="tab" aria-controls="home" aria-selected="true" href="#firstTab">Tất cả</a></li>
        <li class="nav-item"><a class="nav-link" id="profile-tab" data-toggle="tab" aria-controls="profile" aria-selected="false" href="#secondTab">Thêm mới</a></li>


    </ul>
    <div class="tab-content">
        <div id="firstTab" class="tab-pane active" role="tabpanel">@Html.Action("ViewAll")</div>

        <div id="secondTab" class="tab-pane fade in " role="tabpanel">@Html.Action("AddOrEdit")</div>

    </div>
</div>


